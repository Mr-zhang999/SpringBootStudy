<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tongji.demo.mapper.UserMapper">

    <insert id="addUser" parameterType="com.tongji.demo.bean.Person">
        insert into my_study.student(student_id,name,birthday,sexy)
        values (#{Id},#{name},#{birthday},#{sexy})
    </insert>

</mapper>